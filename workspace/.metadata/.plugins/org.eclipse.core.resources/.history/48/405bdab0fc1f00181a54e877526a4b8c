package agerate.mainClasses;

import java.io.File;
import java.io.FileNotFoundException;
import java.util.Scanner;

public class ReadFileByFile {
	public static void main(String[] args) throws FileNotFoundException {
		 
		  Scanner entireFileText = new Scanner(new File("C:/Users/Rian.Rian/Documents/GitHub/rtavares/filme/[rec].html")).useDelimiter("\\A").next();
		 
		  System.out.println(entireFileText.toString());
		 }
}
